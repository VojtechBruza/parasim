<parasim>
	<extension qualifier="remote">
		<property name="host">anna</property>
	</extension>

	<extension qualifier="application">
		<property name="warmupComputationSize">20</property>
		<property name="computationSize">40</property>
		<property name="warmupIterationLimit">3</property>
	</extension>

	<extension qualifier="computation-lifecycle">
		<property name="numberOfThreads">2</property>
		<property name="defaultExecutor">org.sybila.parasim.computation.lifecycle.api.DistributedMemoryExecutor</property>
		<property name="nodes">
			<value>pheme01</value>
			<value>pheme02</value>
			<value>pheme03</value>
			<value>pheme04</value>
		</property>
	</extension>
</parasim>

