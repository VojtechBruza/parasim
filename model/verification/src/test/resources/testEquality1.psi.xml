                                </predicate>
                            </freeze>
                            <predicate>
                                <variable multiplier="8" frozen="1">y</variable>
                                <variable multiplier="-3">y</variable>
                                <equals/>
                                <value>0.889</value>
                            </predicate>
                        </and>
                    </globally>
