            </not>
            <freeze index="2">
                <future>
                    <interval>
                        <lower type="closed">1</lower>
                        <upper type="closed">5</upper>
                    </interval>
                    <predicate>
                        <variable multiplier="0.25" frozen="0">x</variable>
                        <variable multiplier="2.5" frozen="2">z</variable>
