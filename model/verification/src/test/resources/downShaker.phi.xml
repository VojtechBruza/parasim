<formula xmlns="http://www.sybila.org/parasim/stl-formula">
    <freeze index="1">
        <and>
            <not>
                <until>
                    <interval>
                        <lower type="closed">23.1</lower>
                        <upper type="closed">90.1</upper>
                    </interval>
                    <freeze index="1">
                        <or>
                            <predicate>
                                <variable multiplier="27.1">x</variable>
                                <variable multiplier="8.1" frozen="1">z</variable>
                                <variable multiplier="41.9" frozen="3">z</variable>
                                <greater/>
                                <value>18.5</value>
                            </predicate>
                            <not>
                                <future>
                                    <interval>
                                        <lower type="closed">57.9</lower>
                                        <upper type="closed">78.9</upper>
                                    </interval>
                                    <predicate>
                                        <variable multiplier="55.9">y</variable>
                                        <variable multiplier="-13.2" frozen="1">y</variable>
                                        <lesser/>
                                        <value>68.5</value>
                                    </predicate>
                                </future>
                            </not>
                        </or>
                    </freeze>
                    <not>
                        <predicate>
                            <variable multiplier="-90.5">z</variable>
                            <greater/>
                            <value>-41.3</value>
                        </predicate>
                    </not>
                </until>
            </not>
            <freeze index="2">
                <globally>
                    <interval>
                        <lower type="closed">17.6</lower>
                        <upper type="closed">39.2</upper>
                    </interval>
                    <freeze index="3">
                        <and>
                            <freeze index="2">
                                <future>
                                    <interval>
                                        <lower type="closed">39.2</lower>
                                        <upper type="closed">52</upper>
                                    </interval>
                                    <predicate>
                                        <variable multiplier="65.7">x</variable>
                                        <variable multiplier="-74.8" frozen="1">x</variable>
                                        <variable multiplier="62" frozen="3">y</variable>
                                        <lesser/>
                                        <value>-15.5</value>
                                    </predicate>
                                </future>
                            </freeze>
                            <not>
                                <predicate>
                                    <variable multiplier="-14.9">y</variable>
                                    <variable multiplier="98.6" frozen="3">y</variable>
                                    <variable multiplier="46" frozen="1">z</variable>
                                    <lesser/>
                                    <value>-4.1</value>
                                </predicate>
                            </not>
                        </and>
                    </freeze>
                </globally>
            </freeze>
        </and>
    </freeze>
</formula>
